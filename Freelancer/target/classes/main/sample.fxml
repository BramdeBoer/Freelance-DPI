<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.Controller">
    <ComboBox fx:id="cbSector" prefHeight="31.0" prefWidth="221.0" GridPane.columnIndex="1" />
    <TextField fx:id="tfName" GridPane.columnIndex="1" GridPane.rowIndex="1" />
    <TextArea fx:id="taSkillset" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="3" />
    <Label prefHeight="21.0" prefWidth="93.0" text="Sector" />
    <Label prefHeight="21.0" prefWidth="130.0" text="Name" GridPane.rowIndex="1" />
    <Label prefHeight="45.0" prefWidth="149.0" text="Skillsets (comma separated)" wrapText="true" GridPane.rowIndex="3" />
   <TextField fx:id="tfRate" GridPane.columnIndex="1" GridPane.rowIndex="2" />
   <Label prefHeight="21.0" prefWidth="150.0" text="Rate" GridPane.rowIndex="2" />
   <Label fx:id="lbFeedback" minHeight="-Infinity" minWidth="-Infinity" prefHeight="21.0" prefWidth="216.0" text="-" GridPane.columnIndex="1" GridPane.rowIndex="4" />
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
</GridPane>
